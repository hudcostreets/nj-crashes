{
  "name": "njsp-crashes-lambda",
  "version": "1.0.0",
  "main": "njsp-crashes.js",
  "scripts": {
    "build": "esbuild njsp-crashes.ts --bundle --platform=node --target=node18 --outfile=dist/njsp-crashes.js --external:aws-sdk --external:mock-aws-s3 --external:nock --external:better-sqlite3 --external:react --external:react-dom --external:react-plotly.js --external:plotly.js --external:next --format=cjs",
    "pkg": "npm run build && cd dist && zip -r ../njsp-crashes.zip .",
    "create": "./create-lambda.sh",
    "deploy": "aws lambda update-function-code --function-name njsp-crashes --zip-file fileb://njsp-crashes.zip"
  },
  "dependencies": {
    "@rdub/base": "^0.8.0",
    "aws-lambda": "^1.0.7",
    "better-sqlite3": "^12.2.0",
    "duckdb": "^1.3.1",
    "duckdb-async": "^1.3.1",
    "fp-ts": "^2.16.10",
    "plotly.js": "^3.0.1",
    "react-plotly.js": "^2.6.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.0.0",
    "@types/plotly.js": "^3.0.2",
    "@types/react-plotly.js": "^2.6.3",
    "esbuild": "^0.20.2",
    "typescript": "^5.0.0"
  }
}
